# Aladdin HackerRank 


    magicUnits = 0                              
    distanceToGo = 0                            
    lengthMagic = len(magic)                   
    lengthDistance = len(distance)                     
    
    for m in range(start,lengthMagic,1):
        magicUnits = magic[m]                       
        for d in range(start,lengthDistance,1):  
            distanceToGo = distance[d]              
      
            magicUnits = magicUnits - distanceToGo
            distanceToGo = distance[start + 1]
            magicUnits = magicUnits + magic[start + 1] 
    
    
       
    if magicUnits >= distanceToGo:
        return True
    if magicUnits <= distanceToGo:
        return False